{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "@vue/standard"
  ],
  "rules": {
    "camelcase": 0,
    "comma-dangle": 0,
    "no-sequences": 0,
    "no-return-assign": 0,
    "object-property-newline": 0,
    "vue/return-in-computed-property": 0
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}